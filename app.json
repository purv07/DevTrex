{
  "expo": {
    "name": "DevTrex",
    "slug": "devtrex-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new-logo.png",
    "scheme": "devtrex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/new-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5",
      "hideExpoLoadingScreen": false
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/new-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#4F46E5"
      }
    },
    "android": {
      "splash": {
        "image": "./assets/images/new-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#4F46E5"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/new-logo.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-auth-session",
        {
          "schemes": ["devtrex"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}